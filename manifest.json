{
    "name": "NoHate",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://web.whatsapp.com/"],
            "js": ["jquery.min.js", "content.js"]
        }
    ],
    "browser_action":  {
        "default_popup": "popup.html"
    },
    "permissions": ["http://localhost/*"]
}